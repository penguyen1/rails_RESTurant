<div class="form">

  <p>Log In here!</p>
  <% #TODO what's a form_tag for %>
  <%= form_for(:session, url:login_path) do |f| %>
    <%= f.label :username %>:
    <%= f.text_field :username %>
    <br>
    <%= f.label :password %>:
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit "Login"%>
  <% end %>
  <p>New user? <%= link_to "Sign up now!", signup_path %></p>

</div>